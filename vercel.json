{
  "version": 2,
  "builds": [
    {
      "src": "api/main.py",
      "use": "@vercel/python"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "index-main.html",
      "use": "@vercel/static"
    },
    {
      "src": "images/(.*)",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/main.py"
    },
    {
      "src": "/images/(.*)",
      "dest": "/images/$1"
    },
    {
      "src": "/index-main.html",
      "dest": "/index-main.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}

